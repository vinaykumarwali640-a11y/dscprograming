#include <stdio.h>

int main() {
    int src[5], dest[5], i;

    // Reading source array elements
    printf("Enter 5 elements of source array:\n");
    for (i = 0; i < 5; i++) {
        scanf("%d", &src[i]);
    }

    // Copying elements
    for (i = 0; i < 5; i++) {
        dest[i] = src[i];
    }

    // Printing destination array
    printf("Destination array elements:\n");
    for (i = 0; i < 5; i++) {
        printf("%d ", dest[i]);
    }

    return 0;
}
